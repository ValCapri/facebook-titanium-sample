    <android xmlns:android="http://schemas.android.com/apk/res/android">
        <manifest android:versionCode="1" android:versionName="1.0"
            package="com.testapp" xmlns:android="http://schemas.android.com/apk/res/android">
            <uses-sdk android:minSdkVersion="10" android:targetSdkVersion="19"/>
            <application android:debuggable="false"
                android:icon="@drawable/appicon" android:label="TestApp"
                android:name="TestappApplication" android:theme="@style/Theme.AppCompat">
                <activity
                    android:configChanges="keyboardHidden|orientation|screenSize"
                    android:label="@string/app_name"
                    android:name=".TestappActivity" android:theme="@style/Theme.Titanium">
                    <intent-filter>
                        <action android:name="android.intent.action.MAIN"/>
                        <category android:name="android.intent.category.LAUNCHER"/>
                    </intent-filter>
                </activity>
                <activity
                    android:configChanges="keyboardHidden|orientation|screenSize" android:name="org.appcelerator.titanium.TiActivity"/>
